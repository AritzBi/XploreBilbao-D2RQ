# Table building_comments
map:building_comments a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "building_comments/@@building_comments.id@@";
	d2rq:class sioc:Post;
	.
map:building_comments__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:building_comments;
	d2rq:property rdfs:label;
	d2rq:pattern "building_comments #@@building_comments.id@@";
	.
map:building_comments_comment a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:building_comments;
	d2rq:property sioc:content;
	d2rq:column "building_comments.comment";
	.
map:building_comments_note a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:building_comments;
	d2rq:property xplore:note;
	d2rq:propertyDefinitionLabel "building_comments note";
	d2rq:column "building_comments.note";
	d2rq:datatype xsd:double;
	.
map:building_comments_building_id__ref a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:building_comments;
	d2rq:property vocab:building_comments_building_id;
	d2rq:refersToClassMap map:emblematic_building;
	d2rq:join "building_comments.building_id => emblematic_building.id";
	.




